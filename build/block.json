{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/scrolling-text",
  "version": "0.1.0",
  "title": "Scrolling Text",
  "category": "widgets",
  "icon": "editor-bold",
  "description": "A stylable page wide scrolling text",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "text": {
      "type": "string"
    },
    "speed": {
      "type": "string",
      "default": "1"
    }
  },
  "textdomain": "scrolling-text",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}